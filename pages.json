{
	"easycom": {
			"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
    "pages": [
		
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom",
				"backgroundColorBottom":"#F8F8F8"
			}
		},
		{
		    "path" : "pages/login/login",
		    "style" : {
				"navigationBarBackgroundColor":"#FFFFFF",
				"backgroundColor":"#fff"
			}
		},
		
		{
		    "path" : "pages/city/city",
		    "style" : {
				"navigationStyle":"custom"
			}
		},
		{
		    "path" : "pages/contentdetail/contentdetail",
		    "style" : {
				"navigationStyle":"custom"
			}
		},
		
		{
			"path": "pages/attractionsList/attractionsList",
			"style": {
				"navigationStyle": "custom"
			}
		},
		
		
		{
		    "path" : "pages/mine/mine",
		    "style" : {
				"navigationStyle":"custom"
				
			}
		}
		,
		
		{
			"path": "pages/searchResults/searchResults",
			"style": {
				"navigationStyle": "custom"
			}
		},

		{
         "path" : "pages/search/search",
            "style" : {
				"navigationStyle":"custom",
				"mp-baidu":{
					"disableScroll":true
				}
				
			}
        }
        ,
		{
		 "path" : "pages/positionContent/positionContent",
		    "style" : {
				"navigationStyle":"custom"
			}
		}
        
    ],
	"globalStyle": {
		"navigationBarBackgroundColor": "#F7F7F7",
		"navigationBarTextStyle": "black"
	},
	"tabBar": {
		"list": [{
				"pagePath": "pages/index/index",
				"text": "首页",
				"iconPath": "/static/images/index.png",
				"selectedIconPath": "/static/images/index-active.png"
			},
			{
				"pagePath": "pages/mine/mine",
				"text": "我的",
				"iconPath": "/static/images/mine.png",
				"selectedIconPath": "/static/images/mine-active.png"
			}
		],
		"backgroundColor": "#ffffff",
		"color": "#C9CAD1",
		"selectedColor": "#303133",
		"borderStyle":"black"
	},
	"dynamicLib": {
		"smartUI": {
			"provider": "smart-ui"
		}
	}
}
