	"preloadRule":{
		"pages/index/index": {
		    "network": "all",
		    "packages": ["pages_content","pages_search","pages_mine"]
		},
        "pages/mine/mine": {
            "network": "all",
            "packages": ["pages_mine"]
        },
        "pages_content/article/article": {
            "network": "all",
            "packages": ["pages_article"]
        },
        "pages_content/provinces/provinces": {
            "network": "all",
            "packages": ["pages_province", "pages_questions"]
        },
		"pages_mine/mineAnswers/mineAnswers": {
            "network": "all",
            "packages": ["pages_questions"]
        }
    },
