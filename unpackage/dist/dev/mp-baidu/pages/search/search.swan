<view class="content"><view class="search-box"><m-search class="mSearch-input-box" vue-id="50c90251-1" mode="{{2}}" button="inside" placeholder="{{defaultKeyword}}" value="{{keyword}}" data-event-opts="{{[['^search',[['doSearch',[false]]]],['^input',[['__set_model',['','keyword','$event',[]]],['inputChange']]],['^confirm',[['doSearch',[false]]]]]}}" bind:search="__e" bind:input="__e" bind:confirm="__e"></m-search></view><view class="search-keyword"><scroll-view hidden="{{!(isShowKeywordList)}}" class="keyword-list-box" scroll-y="{{true}}"><block s-for="{{keywordList}}" s-for-item="row" s-for-index="index" s-key="index"><block><view class="keyword-entry" hover-class="keyword-entry-tap"><view data-event-opts="{{[['tap',[['doSearch',['$0'],['keywordList.'+index+'.keyword']]]],['tap',[['Toresults',['$event']]]]]}}" class="keyword-text" catchtap="__e" bindtap="__e"><rich-text nodes="{{row.htmlStr}}"></rich-text></view><view data-event-opts="{{[['tap',[['setKeyword',['$0'],['keywordList.'+index+'.keyword']]]]]}}" class="keyword-img" catchtap="__e"><image src="/static/images/back.png"></image></view></view></block></block><view class="search-bottom">{{'搜索更多关于"'+keyword+'"的结果'}}</view></scroll-view><scroll-view hidden="{{!(!isShowKeywordList)}}" class="keyword-box" scroll-y="{{true}}"><block s-if="{{oldKeywordList.length>0}}"><view class="keyword-block"><view class="keyword-list-header"><view>历史记录</view><view><image src="/static/images/delete.png" data-event-opts="{{[['tap',[['oldDelete',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="keyword"><block s-for="{{oldKeywordList}}" s-for-item="keyword" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['doSearch',['$0'],[[['oldKeywordList','',index]]]]]]]}}" bindtap="__e">{{keyword}}</view></block></view></view></block><view class="keyword-block"><view class="keyword-list-header"><view>热门搜索</view><view><image src="{{'/static/images/attention'+forbid+'.png'}}" data-event-opts="{{[['tap',[['hotToggle',['$event']]]]]}}" bindtap="__e"></image></view></view><block s-if="{{forbid==''}}"><view class="keyword"><block s-for="{{hotKeywordList}}" s-for-item="keyword" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['doSearch',['$0'],[[['hotKeywordList','',index]]]]]]]}}" bindtap="__e">{{keyword}}</view></block></view></block><block s-else><view class="hide-hot-tis"><view>当前搜热门搜索已隐藏</view></view></block></view></scroll-view></view></view>